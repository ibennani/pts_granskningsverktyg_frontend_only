{
  "name": "modulbaserad-kod-v02",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:fixedport": "vite --port 5173 --strictPort",
    "build": "vite build && node scripts/postbuild-copy.js",
    "preview": "vite preview --open",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "PWDEBUG=1 playwright test",
    "watch:e2e": "concurrently -k -n DEV,TEST \"npm:dev:fixedport\" \"chokidar 'src/**/*' -c 'playwright test' --throttle 200\"",
    "postinstall": "playwright install"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.0.1",
    "vite": "^7.1.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "codex": "^0.2.3",
    "jsdom": "^27.0.0"
  }
}
